{
  "version": 2,
  "name": "thc-packaging-and-labeling-requirements",
  "routes": [
    { "handle": "filesystem" },
    { "src": "^/studio/static/(.*)", "dest": "/studio/static/$1" },
    { "src": "^/studio/(.*)", "dest": "/studio/index.html" },
    {
      "src": "/(.*)", "dest": "/web/$1"
    }
  ]
}